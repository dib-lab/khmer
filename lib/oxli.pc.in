prefix=@prefix@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
sharedlibdir=${libdir}
includedir=${prefix}/include

Name: oxli
Description: The unsupported core C++ library from the khmer project
URL: http://khmer.readthedocs.io/
Version: @VERSION@

Requires:
Libs: -L${libdir} -L${sharedlibdir} -loxli
Cflags: -I${includedir}
